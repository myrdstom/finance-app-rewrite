{
  "name": "finance-app-rewrite",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.3",
    "@mui/system": "^5.11.2",
    "@mui/x-data-grid": "^5.17.23",
    "@redux-devtools/extension": "^3.2.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.2.2",
    "dayjs": "^1.11.7",
    "eslint-plugin-cypress": "^2.12.1",
    "formik": "^2.2.9",
    "highcharts": "^10.3.2",
    "highcharts-react-official": "^3.1.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testPathPattern src/features --env=jsdom --coverage jest --silent",
    "one-test": "react-scripts test --testPathPattern src/features/loans/addLoan/addLoan.test.js --env=jsdom jest",
    "cypress": "cypress open",
    "eject": "react-scripts eject",
    "lint:js": "eslint src --fix",
    "lint:check-js": "eslint src",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint:js"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "cypress": "^12.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest-matchmedia-mock": "^1.1.0",
    "lint-staged": "^13.1.0",
    "msw": "^1.0.1",
    "prettier": "^2.8.2",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "coveragePathIgnorePatterns": [
      "/src/App.js",
      "/src/index.js",
      "/src/pages",
      "/src/reportWebVitals.js",
      "./src/__tests__/__fixtures__/*"
    ]
  }
}
